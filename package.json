{
  "name": "scss-theming",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "build:lib": "ng build components && npm run build:lib:scss",
    "build:lib:scss": "npm run build:lib:default-scss && npm run build:lib:custom-scss",
    "build:lib:default-scss": "scss-bundle --project projects/components --entryFile projects/components/scss/default-theme/default-theme.scss --outFile dist/components/styles/default-theme.scss",
    "build:lib:custom-scss": "scss-bundle --project projects/components --entryFile projects/components/scss/custom-theme/custom-theme.scss --outFile dist/components/styles/custom-theme.scss"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.0.1",
    "@angular/common": "9.0.1",
    "@angular/compiler": "9.0.1",
    "@angular/core": "9.0.1",
    "@angular/forms": "9.0.1",
    "@angular/platform-browser": "9.0.1",
    "@angular/platform-browser-dynamic": "9.0.1",
    "@angular/router": "9.0.1",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.900.2",
    "@angular-devkit/build-ng-packagr": "~0.900.2",
    "@angular/cli": "9.0.2",
    "@angular/compiler-cli": "9.0.1",
    "@angular/language-service": "9.0.1",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jasmine": "3.5.4",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "13.7.1",
    "angular-tslint-rules": "1.20.3",
    "codelyzer": "5.2.1",
    "husky": "4.2.3",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.1",
    "karma-jasmine": "3.1.1",
    "karma-jasmine-html-reporter": "1.5.2",
    "karma-junit-reporter": "2.0.1",
    "karma-spec-reporter": "0.0.32",
    "lint-staged": "10.0.7",
    "ng-packagr": "^9.0.0",
    "protractor": "5.4.3",
    "scss-bundle": "3.0.2",
    "ts-node": "8.6.2",
    "tslint": "6.0.0",
    "typescript": "3.7.5"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
